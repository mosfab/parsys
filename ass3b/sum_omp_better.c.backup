#include <stdio.h>

#define N 1000000000


int main(){
	unsigned long long s=0;
	unsigned long long j = 0;
        #pragma omp parallel for reduction(+:s) private(j)
                for(unsigned long long i = 1; i<=N; i++){
			j=i;
                        s=s+j;
                }
        printf("%llu\n", s);
}

